{
  "name": "react-next-boilerplate",
  "version": "1.4.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "generate": "pnpm plop --plopfile generators/plopfile.js",
    "lint": "pnpm lint:all",
    "lint:next": "next lint",
    "lint:js": "eslint ./src --max-warnings=0",
    "lint:all": "pnpm lint:next && pnpm lint:css ",
    "lint:css": "stylelint './src/**/*.ts' --max-warnings 0",
    "prepare": "husky install",
    "start": "next start",
    "test:ci": "jest --runInBand",
    "test:watch": "jest --watch --maxWorkers=25%",
    "test": "jest --maxWorkers=50% --passWithNoTests",
    "storybook:watch": "storybook dev -p 6006",
    "storybook:build": "storybook build"
  },
  "dependencies": {
    "next": "13.5.2",
    "next-pwa": "^5.6.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "styled-components": "^6.0.8",
    "styled-media-query": "^2.1.2"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.22.15",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@storybook/addon-a11y": "^7.4.3",
    "@storybook/addon-essentials": "^7.4.3",
    "@storybook/blocks": "^7.4.3",
    "@storybook/csf-tools": "^7.4.3",
    "@storybook/nextjs": "^7.4.3",
    "@storybook/react": "^7.4.3",
    "@storybook/testing-library": "^0.2.1",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@total-typescript/ts-reset": "^0.5.1",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/jest": "^29.5.5",
    "@types/node": "20.6.3",
    "@types/react": "18.2.22",
    "@types/react-dom": "18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "babel-plugin-styled-components": "^2.1.4",
    "eslint": "8.49.0",
    "eslint-config-next": "13.5.2",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-storybook": "^0.6.13",
    "fs-extra": "^11.1.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-styled-components": "^7.1.1",
    "lint-staged": "^14.0.1",
    "plop": "^3.1.2",
    "postcss-styled-syntax": "^0.5.0",
    "prettier": "3.0.3",
    "storybook": "^7.4.3",
    "stylelint": "^15.10.3",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-order": "^6.0.3",
    "typescript": "5.2.2"
  }
}